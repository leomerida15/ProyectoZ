<!-- Login -->
<div class="col s12 m6">
	<form action="/login" method="POST" id="login">
		<div class="card blue-grey darken-1">
			<div class="card-content white-text">
				<span class="card-title">login</span>
				<div class="input-field">
					<input name="correo_login" id="correo_login" type="email" class="validate" required />
					<label for="correo_login">Correo</label>
				</div>

				<div class="input-field">
					<input name="clave_login" id="clave_login" type="password" class="validate" required />
					<label for="clave_login">clave</label>
				</div>
			</div>
			<div class="card-action">
				<button class="btn waves-effect waves-light right" type="submit" id="asubmit">
					Submit
					<i class="material-icons right">send</i>
				</button>
			</div>
		</div>
	</form>
</div>
