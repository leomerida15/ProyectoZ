<!-- Modal Trigger -->
<div class="container">
	<% images.forEach( image => { %>
	<div id="<%=image.name%>" class="modal modal-fixed-footer">
		<div class="modal-content row">
			<div class="col s12 m2"></div>
			<div class="col s12 m6">
				<div class="center-align">
					<img src="img/<%=image.ubicacion%>" class="responsive-img materialboxed" alt="" />
				</div>
			</div>
			<div class="col s12 m4">
				<div class="section">
					<table>
						<thead>
							<tr>
								<th>Formas de Descarga</th>
							</tr>
						</thead>

						<tbody>
							<tr>
								<td>
									<a class="light-blue darken-4 waves-effect waves-light btn small col s12"><i class="material-icons right">file_download</i>Gratis</a>
								</td>
							</tr>

							<tr>
								<td>
									<a class="btn-floating btn-large waves-effect waves-light light-blue lighten-1 tooltipped" data-position="bottom" data-tooltip="<%=image.si%>">
										<i class="material-icons">thumb_up</i>
									</a>
									<a class="btn-floating btn-large waves-effect waves-light red lighten-1 tooltipped" data-position="bottom" data-tooltip="<%=image.no%>">
										<i class="material-icons">thumb_down</i>
									</a>
								</td>
							</tr>
						</tbody>
					</table>
				</div>
			</div>
			<% datos=image.datos%>
			<div class="col s12">
				<div class="card blue-grey darken-1 container">
					<div>
						<% datos.forEach( dato => { %>

						<div class="card-content white-text row">
							<div class="col s3">
								<i class="material-icons prefix col s3">face</i>
								<p class="col s9"><%= dato.usuario%></p>
							</div>
							<div class="col s9">
								<p><%= dato.mensaje%></p>
							</div>
						</div>

						<%})%>
					</div>
				</div>
			</div>
		</div>
		<div class="modal-footer">
			<a href="#!" class="modal-action modal-close waves-effect waves-green btn-flat">cerrar</a>
		</div>
	</div>

	<% }) %>
</div>
