<div class="col s12 m4">
	<div class="card blue-grey darken-1 container">
		<div class="card-content white-text row">
			<div id="opinion<%=value%>">
				<div class="col s6">
					<form action="" method="POST" id="si<%=value%>">
						<input class="white-text hide" type="text" value="<%=user.usuario%>" name="usuario_gusta" />
						<input class="white-text hide" type="text" value="<%=image.name%>" name="name_gusta" />
						<input class="white-text hide" type="text" value="si" name="gusta" />
						<button type="submit" class="btn-floating btn-large waves-effect waves-light light-blue lighten-1 tooltipped" data-position="bottom" data-tooltip="<%=image.si%>">
							<i class="material-icons">thumb_up</i>
						</button>
					</form>
				</div>
				<div class="col s6">
					<form action="" method="POST" id="no<%=value%>">
						<input class="white-text hide" type="text" value="<%=user.usuario%>" name="usuario_gusta" />
						<input class="white-text hide" type="text" value="<%=image.name%>" name="name_gusta" />
						<input class="white-text hide" type="text" value="no" name="gusta" />
						<button type="submit" class="btn-floating btn-large waves-effect waves-light red lighten-1 tooltipped" data-position="bottom" data-tooltip="<%=image.no%>">
							<i class="material-icons">thumb_down</i>
						</button>
					</form>
				</div>
			</div>
			<div class="col s12">.</div>
			<div class="col s12">
				<a class="section light-blue darken-4 waves-effect waves-light btn small col s12"><i class="material-icons right">file_download</i>Gratis</a>
			</div>
			<div class="col s12">.</div>
			<div class="col s12">
				<a class="sectionlight-blue darken-4 waves-effect waves-light btn small col s12"><i class="material-icons right">add_shopping_cart</i>Carrito</a>
			</div>
		</div>
	</div>
</div>
